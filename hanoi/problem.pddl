(define (problem p1)
	(:domain hanoi)
	(:objects k1 k2 k3 x y z)
	(:init
		(clear k1)
		(clear y)
		(clear z)
		(on k1 k2)
		(on k2 k3)
		(on k3 x)
		(smaller k1 k2)
		(smaller k1 k3)
		(smaller k1 x)
		(smaller k1 y)
		(smaller k1 z)
		(smaller k2 k3)
		(smaller k2 x)
		(smaller k2 y)
		(smaller k2 z)
		(smaller k3 x)
		(smaller k3 y)
		(smaller k3 z)
		
	)
	(:goal
		(and
			(on k1 k2)
			(on k2 k3)
			(on k3 z)
			(clear x)
			(clear y)
		)
	)
)